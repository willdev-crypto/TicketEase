{
  "name": "ticketease",
  "version": "1.0.0",
  "description": "Aplicativo PWA para gerenciar chamados",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    
  },
  "devDependencies": {
    "electron": "^35.0.1",
    "electron-builder": "^23.1.0"
  },
  "build": {
    "appId": "com.ticketease.app",
    "productName": "TicketEase",
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
